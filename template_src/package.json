{
  "name": "vcwf-template",
  "version": "1.4.0",
  "description": "A template that allows you to create a project preconfigured with Vue + Framework7 + Webpack + Cordova for iOS or Android Apps.",
  "scripts": {
    "add:platforms": "cordova platform add android browser ios",
    "build": "npm run clean && webpack",
    "build:android": "npm run build && cordova build android",
    "build:cordova": "cordova build",
    "clean": "rimraf www",
    "clean:all": "rimraf www platforms node_modules",
    "dev:android": "cordova run android -- --lr --device",
    "dev:browser": "cordova run browser -- --lr",
    "dev:ios": "cordova run ios -- --lr",
    "init": "npm install && npm run build && npm run add:platforms && npm run dev:browser"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "license": "ISC",
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^1.6.1",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.28.4",
    "current-device": "^0.6.0",
    "epipebomb": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.19.0",
    "svg-url-loader": "^2.0.2",
    "uglify-js": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.6.2",
    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^3.3.0",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "cordova-android": "^7.0.0",
    "cordova-browser": "^5.0.3",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-whitelist": "^1.3.3",
    "framework7": "^2.0.6",
    "framework7-vue": "^2.0.0",
    "loglevel": "^1.4.1",
    "vue": "^2.5.13",
    "vue2-google-maps": "^0.8.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "browser",
      "ios"
    ]
  }
}